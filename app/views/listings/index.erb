<h2>Listings</h2>
<h3> <%= render 'errors' %>
 </h3>
<br>
<%= form_tag(listings_path, method: :get) do %>
  Search for a listing by state <br>
  <%= text_field_tag :state, nil, placeholder: "NY, CA, etc." %>  <%= submit_tag 'Search'  %> <br> <br>
<% end %>

<% @listings.each do |listing| %>
  <%= link_to listing.name, listing %><br><br>
<% end %>
