<% if flash[:error] %>
  <% flash[:error].each do |e| %>
  <%= e %><br><br>
  <% end %>
<% end %>

<%= form_for @listing do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :state %>
  <%= select_tag "state", options_for_select(@states) %>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <!-- <%= f.label :user %> -->
  <!-- <%= f.collection_select :user_id, User.all, :id, :name %><br> -->
  <label>Upload a photo of your listing:</label>
   <%= f.file_field :image %><br><br>
  <%= f.submit %>
<% end %>
